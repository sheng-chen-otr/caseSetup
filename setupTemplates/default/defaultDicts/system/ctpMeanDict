cptMean
{
    // Mandatory entries (unmodifiable)
    type            pressure;
    libs            (fieldFunctionObjects);

    // Mandatory entries (runtime modifiable)
    mode            totalCoeff;

    // Optional entries (runtime modifiable)
    p               pMean;
    U               UMean;
    rho             rhoInf;
    rhoInf          1.205; // enabled if rho=rhoInf
    pRef            0.0;
    hydroStaticMode none;
    result ctpMean;
    
    pInf            0.0;
	UInf            (<INLET_MAG> 0 0); 
}
