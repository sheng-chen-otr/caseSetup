FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     potentialFoam;
startFrom       latestTime; //startTime
startTime       0;
stopAt          endTime;
endTime         500;
deltaT          1;
writeControl    runTime;
writeInterval   500;
purgeWrite      1;
writeFormat     binary;
writePrecision  6;
writeCompression on;
timeFormat      general;
timePrecision   6;
runTimeModifiable yes;
functions
{
	#include "forceCoeffs"
	#include "averageFieldsDict"	
	fieldMinMax
	{
		// Mandatory entries (unmodifiable)
		type        fieldMinMax;
		libs        (fieldFunctionObjects);
		mode        magnitude;
		fields      (U p k omega);
		location    true;
		writePrecision  6;
		writeToFile     false;
		useUserTime     true;
		region          region0;
		enabled         true;
		log             true;
		executeControl  timeStep;
		executeInterval 1;
		writeControl    timeStep;
		writeInterval   1;
	}
}   


