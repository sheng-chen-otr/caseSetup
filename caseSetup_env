#line says where this script is and sets the directory
#caseSetupDir=$( cd -- "$( dirname -- "${BASH_SOURCE[0]}" )" &> /dev/null && pwd )
caseSetupDir="$( cd "$( dirname "${BASH_SOURCE[0]}" )" && pwd )"
echo $caseSetupDir
#runs the python that is included in this program
caseSetupPy="$caseSetupDir/python/python3.8/bin/python3.8"

#runs this version of caseSetupDir
alias caseSetup="$caseSetupDir/python/python3.8/bin/python3 $caseSetupDir/caseSetupScript.py"


