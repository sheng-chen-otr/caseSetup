FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     SRFSimplefoam;
startFrom       <STARTFROM>; //startTime
startTime       <STARTTIME>;
stopAt          endTime;
endTime         <ENDTIME>;
deltaT          <DT>;
writeControl    runTime;
writeInterval   <WRITEINT>;
purgeWrite      <PURGEINT>;
writeFormat     binary;
writePrecision  6;
writeCompression off;
timeFormat      general;
timePrecision   6;
runTimeModifiable yes;
functions
{
	#include "forceCoeffs"
	#include "averageFieldsDict"	
	fieldMinMax
	{
		// Mandatory entries (unmodifiable)
		type        fieldMinMax;
		libs        (fieldFunctionObjects);
		mode        magnitude;
		fields      (U Urel p k omega);
		location    true;
		writePrecision  6;
		writeToFile     false;
		useUserTime     true;
		region          region0;
		enabled         true;
		log             true;
		executeControl  timeStep;
		executeInterval 1;
		writeControl    timeStep;
		writeInterval   1;
	}
}   


