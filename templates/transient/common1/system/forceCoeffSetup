type            forceCoeffs;
libs            ("libforces.so");
writeControl    timeStep;
log             no;
rho             rhoInf;      // Indicates incompressible
rhoInf          1;           // Redundant for incompressible
liftDir         (0 0 1);
dragDir         (1 0 0);
CofR            (<REFCOR>);  // Axle midpoint on ground
pitchAxis       (0 1 0);
magUInf         <INLETMAG>; 
lRef            <REFLEN>;        // Wheelbase length
Aref            <REFAREA>;        // Estimated
porosity	    <POROSITY>;

